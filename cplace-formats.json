    "cplace" : {
        "title" : "Common cPlace Log",
        "description" : "The default log format for cplace applications",
        "file-pattern" : ".*cplace.*",
        "multiline" : true,
        "regex" : {
            "cplace" : {
                "pattern" : "^(?<timestamp>\\d{4}-\\d{2}-\\d{2}\\s\\d{2}:\\d{2}:\\d{2},\\d{3})\\s\\[(?<thread>[^\\]]*)\\]\\sC:(?<correlation_id>[a-z0-9]*)\\s(?<level>[A-Z]*\\s?)\\s?T:(?<tenant>[^\\s]*)\\s?P:(?<request_path>[^\\s]*)\\s?(?<location>[^\\s]*)\\s\\[(?<file_and_line>(?<file>[^\\:]*):(?<line>[^\\s]*))\\]\\s-\\s(?<message>.*(.*\\n)*)"
            }
        },
        "opid-field" : "correlation_id",
        "level-field" : "level",
        "level" : {
            "error" : "ERROR",
            "warning" : "WARN",
            "info" : "INFO",
            "debug" : "DEBUG"
        },
        "value" : {
           "thread" : {
                "kind" : "string",
                "identifier" : true
            },
            "correlation_id" : {
                "kind" : "string",
                "identifier" : true,
                "description" : "Work related to each other for a request can be found with this id."
            },
            "level" : {
                "kind" : "string",
                "identifier" : true
            },
            "tenant" : {
                "kind" : "string",
                "identifier" : true
            },
            "request_path" : {
                "kind" : "string",
                "identifier" : true
            },
            "location" : {
                "kind" : "string",
                "identifier" : false,
                "description" : "Location where the log was triggered"
            },
            "file" : {
                "kind" : "string",
                "identifier" : true
            }
        },
        "sample" : [
            {
                "line" : "2020-01-08 11:03:22,231 [localhost-test] C:12345 T:/cplace/local/ P:/cplace/api/cf/cplace/api cf.cplace.platform.server.LocalLog [lnav.java:11] - individual text",
                "level" : "info"
            }
        ]
    },
